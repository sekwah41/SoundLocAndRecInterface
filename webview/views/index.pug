- var sounds = 0;
html
    head
        title= title
        script(src='socket.io.js')
        script(src='webview.js')

        link(href='style.css' rel='stylesheet')
    body
        #display.center
            #user.center
            #ring.center
                while sounds < max_sources
                    .soundarm(id= "soundrot" + sounds)
                        .soundinfooffset
                            .soundinfo
                                span.soundtype= `{{${sounds}}}`
                                br
                                span.soundpercent 0%
                    - sounds++;

